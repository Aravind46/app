        function fn_init_modal(type) {
            var cType = $(type).attr('data-type')
            $('.body').fadeOut();
            switch (cType) {
                case "Rcomplaint":
                    $('.complaintList').fadeIn()
                    break;
                case "Scomplaint":
                    $('.Cmp_state').fadeIn()
                    break;
            }


            //$('#General_modal_body').html('');
            //$('#General_modal').modal();

        }
        function Loadhome() {
            $('.complaintList,.Cmp_state').fadeOut()
            $('.body').fadeIn();
        }
        function fnProcessCmpStat() {
            var objectArray = [{
                "Total": "34",
                "Version": "1.0.4vdfg",
                "Version2": "1.0.445",
                "Version3": "1.0.45454",
                "Version4": "1.540.4",
                "Version7": "541.0.4",
                "Office": "New Yorksaddddddddddddddddddddddddddddd ddddddddddddddddddddddddd   ddddddddddddddddddddddddddddd"
            }];
            var txt = ""
            for (var i = 0; i < (Object.keys(objectArray[0]).length); i++) {

                txt += "<tr>"
                txt += "<td>" + Object.keys(objectArray[0])[i] + "</td><td>" + objectArray[0][Object.keys(objectArray[0])[i]] + "</td>"
                try { txt += "<td>" + Object.keys(objectArray[0])[i + 1] + "</td><td>" + objectArray[0][Object.keys(objectArray[0])[i + 1]] + "</td>" }
                catch (err) { console.log('indexOut') };
                txt += "</tr>"

            }
            $('#CmpDetails').html(txt)
        }
