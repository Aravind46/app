        $('#Cmpt_Mobile').keyup(function(){
            
            var Tempval = ($('#Cmpt_Mobile').val()).trim();
            if(isNaN(Tempval)==true){
                Tempval = Tempval.slice(0, Tempval.length-1)
                $('#Cmpt_Mobile').val(Tempval);             
            }

        })
        function Process() {
            var UsrIn = $('#User').val();
            var Text = "<span class='usr'>" + UsrIn + "</span><br>";
            $('.chatArea').append(Text)
$('#User').val("")

        }
