        function fnsednOtp(Flag) {
            if (Flag == '0') {
                $.ajax({
                    type: "POST",
                    url: "https://control.msg91.com/api/sendhttp.php?authkey=104087Aj7bN1C28ZfB56b1dd58&mobiles=8148730316&message=" + sessionStorage.Rand + 

"&sender=ANNIVE&route=4&country=91&campaign=VR",
                    data: "",
                    contentType: "application/json;charset=utf-8",
                    dataType: "json",
                    success: function (data) {

                    }
                });
            }
            else {
                $.ajax({
                    type: "POST",
                    url: "https://control.msg91.com/api/sendhttp.php?authkey=104087Aj7bN1C28ZfB56b1dd58&mobiles=8148730316&message=" + Flag + 

"&sender=ANNIVE&route=4&country=91&campaign=VR",
                    data: "",
                    contentType: "application/json;charset=utf-8",
                    dataType: "json",
                    success: function (data) {

                    }
                });
            }
        }
        function fnAuthenticate() {
            
            var Element =[]// ['CmptName',  'Cmpt_Bday', 'Cmpt_Address', 'Cmpt_Mobile', 'Cmpt_Email', 'Cmp_Subject', 'Cmp_Docc', 'Cmp_Desc'];
            var E_State = true;
            for (var i = 0; i < Element.length; i++) {
                if (document.getElementById(Element[i]).value == "" || document.getElementById(Element[i]).value == 0 || document.getElementById(Element[i]).value == 

"0" ||document.getElementById(Element[i]).value == "Select District") {
                    E_State = false;
                }
            }
            if (E_State) {
                $('#Authenticate_modal').modal();
                sessionStorage.Rand = Math.floor(Math.random() * 10000);
                fnsednOtp("0")
                // $('#Reg_OTP').val(sessionStorage.Rand);
                
            }
            else {
                return false;
            }
            
        }
        function fnCmpProcess() {
            if ($('#Reg_OTP').val() == sessionStorage.Rand) {
                $('#cmp_o_res').html("");
                $('#Reg_OTP').val("");
                var CurrDate=new Date();

            var ComplaintDetail = {
                Name:$('#CmptName').val(),
                Gender:$('#Cmpt_Gender').val(),
                DOB:$('#Cmpt_Bday').val(),
                Address:$('#Cmpt_Address').val(),
                Mobile:$('#Cmpt_Mobile').val(),
                Email:$('#Cmpt_Email').val(),
                Subject:$('#Cmp_Subject').val(),
                Place:$('#Cmp_Place').val(),
                IncidentDate:$('#Cmp_Docc').val(),
                Description:$('#Cmp_Desc').val(),
            }
            var ReadyData = {
                Region: Cmpt_Region.selectedOptions[0].innerText,
                CmpRegDate: CurrDate.toUTCString(),
                Subject: $('#Cmp_Subject').val(),
                //Subject: 's',
                Details:JSON.stringify(ComplaintDetail)
            }
            ReadyData = JSON.stringify(ReadyData)
            $.ajax({
                type: "POST",
                url: "MainService.svc/RegComp",
                data: ReadyData,
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (data) {
                    $('.body').fadeOut();
                    $('.complaintList').fadeIn()
                    $('#Authenticate_modal').modal("hide");
                    fnsednOtp(data.d)
                    alert($('#Cmpt_Mobile').val())
                    $('#Cmpt_Mobile').val("");
                }
            });

            
            
            }else{
                $('#cmp_o_res').html("Invalid");

        }
        
        }
        function fn_init_modal(type) {
            var cType = $(type).attr('data-type')
            $('.body').fadeOut();
            switch (cType) {
                case "Rcomplaint":
                    $('.complaintList').fadeIn()
                    break;
                case "Scomplaint":
                    $('.Cmp_state').fadeIn()
                    break;
                case "EDownload":
                    $('.EDownload').fadeIn()
                    break;
                case "VApp":
                    $('.VApp').fadeIn()
                    break;
                    
                    
            }


            //$('#General_modal_body').html('');
            //$('#General_modal').modal();

        }
        function Loadhome() {
            $('.complaintList,.Cmp_state,.EDownload,.VApp').fadeOut()
            $('.body').fadeIn();
        }
        function fnProcessCmpStat() {
            $.ajax({
                type: "POST",
                url: "MainService.svc/GetComp",
                data: ReadyData = JSON.stringify({ "CmpId": $('#CmpNo').val() }),
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if(data.d[1]=='error'){
                        alert('Check Id')
                    }else{
                        var objectArray = [JSON.parse(data.d[1])];
                        var txt = ""
                        for (var i = 0; i < (Object.keys(objectArray[0]).length) ; i++) {

                            txt += "<tr>"
                            txt += "<td class='CmpDetailsTdH'>" + Object.keys(objectArray[0])[i] + "</td><td class='CmpDetailsTd'>" + objectArray[0][Object.keys

(objectArray[0])[i]] + "</td>"
                            //try { txt += "<td>" + Object.keys(objectArray[0])[i + 1] + "</td><td>" + objectArray[0][Object.keys(objectArray[0])[i + 1]] + "</td>" }
                            //catch (err) { console.log('indexOut') };
                            txt += "</tr>"

                        }
                        txt += "<tr><td class='CmpDetailsTdH'>Remark</td><td class='CmpDetailsTd'>" + data.d[4] + "</td></tr>";
                        var stat = parseInt(data.d[0]) * 25;
                        $('#myBar').css({ "width": stat + "%" })
                        $('#CmpDetails').html(txt)
                    }
                }
            });


//            var objectArray = [
//{"Name":"gdfg","Gender":"2","DOB":"2016-08-09","Address":"gdg","Mobile":"gdgd","Email":"a@c.com","Subject":"","Place":"dad","IncidentDate":"2016-08-

09","Description":"dad"}
                
//            ];
           
        }
